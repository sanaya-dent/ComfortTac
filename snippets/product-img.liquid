    <div data-replace-img2></div>
    <noscript><img data-wpsob="1" data-wpsob-height="1" height="1" data-wpsob-width="1" width="1" class="t4s-product-main-img" loading="lazy" src="{{ image | image_url: width: 600 }}"  alt="{% if image.alt %}{{ image.alt | escape }}{% else %}{{ shop.name }}{% endif %}"></noscript>
